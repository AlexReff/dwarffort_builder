{
    "presets": [
        [
            "minify",
            {
                "builtIns": false,
                "mangle":
                {
                    "topLevel": true
                }
            }
        ],
        [
            "@babel/preset-env",
            {
                "targets": [">0.25%", "not ie 11"],
                "useBuiltIns": "usage"
            }
        ]
        //,[
        //     "@babel/preset-react",
        //     {
        //         "pragma": "h"
        //     }
        // ]
        //"es2015"
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"]
}